@model ClockStore.DTO.Cantact
@using WebApp.Resources.Design;
@{
    ViewBag.Title = "Create";
    Layout = "~/Views/Shared/_Layout.cshtml";
}



@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()

    <div class="contact">
        <div class="container">
            <div class="contact-top heading">
                <h2>@WebApp.Resources.Design.Design.ContactTitle</h2>
            </div>
            <div class="contact-text">
                <div class="col-md-12 contact-right">
                    <form>
                        <input type="text" name="FullName" id="FullName" placeholder="@Design.Name" required="">
                        <input type="text" placeholder="@Design.Phone" name="Phone" id="Phone">
                        <input type="text" placeholder="@Design.Email" name="Email" id="Email" required="">
                        <textarea placeholder="@Design.Message" required="" name="Message" id="Message"></textarea>
                        <div class="submit-btn">
                            <input type="submit" value="@Design.SUBMITMessage">
                        </div>
                    </form>
                </div>


                <div class="clearfix"></div>
            </div>
        </div>
    </div>
    <!--contact-end-->
    <!--map-start-->
    <!--<div class="map">
        <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d6632.248000703498!2d151.265683!3d-33.7832959!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x6b12abc7edcbeb07%3A0x5017d681632bfc0!2sManly+Vale+NSW+2093%2C+Australia!5e0!3m2!1sen!2sin!4v1433329298259" style="border:0"></iframe>
    </div>-->
    <!--map-end-->
}

